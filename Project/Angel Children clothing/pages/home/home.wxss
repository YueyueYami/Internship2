.allGoodbox {
  background-color: #FFF;
}

.allGoodbox .allGoodTit {
  width: 100vw;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background-color: #FFF;
}

.allGoodbox .allGoodTit .allGoodLine {
  width: 160rpx;
  border-top: 1px solid #CCC;
}

.allGoodbox .allGoodTit .allGoodIcon {
  width: 30%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.allGoodbox .allGoodTit .allGoodIcon image {
  width: 60rpx;
  height: 60rpx;
}

.allGoodbox .allGoodbox {
  display: flex;
  flex-wrap: wrap;
  padding: 0 10rpx;
  box-sizing: border-box;
}

.allGoodbox .allGoodbox .goodInfo {
  width: 46%;
  height: 500rpx;
  border-radius: 20rpx;
  margin: 8rpx 20rpx;
  margin-right: 0;
  border: 1px solid #ccc;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.allGoodbox .allGoodbox .goodInfo .goodImg {
  width: 100%;
  height: 320rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

.allGoodbox .allGoodbox .goodInfo .goodName {
  font-size: 28rpx;
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  word-wrap: break-word;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.allGoodbox .allGoodbox .goodInfo .goodIntroduce {
  font-size: 26rpx;
  width: 100%;
  color: #555;
  padding: 0 20rpx;
  box-sizing: border-box;
  word-wrap: break-word;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.allGoodbox .allGoodbox .goodInfo .goodPrice {
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.allGoodbox .allGoodbox .goodInfo .goodPrice .goodNowPrice {
  font-size: 34rpx;
  color: #F00;
}

.allGoodbox .allGoodbox .goodInfo .goodPrice .goodOldPrice {
  font-size: 26rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  text-decoration: line-through;
  color: #555;
}
