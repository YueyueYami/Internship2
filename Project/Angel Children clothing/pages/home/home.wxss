page {
  background-color: #EEE;
}

.search {
  width: 100vw;
  height: 125rpx;
  display: flex;
  background-color: #FFF;
  align-items: flex-end;
  position: fixed;
  z-index: 1;
  overflow: hidden;
}

.search .searchTit {
  width: 30%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 44rpx;
  text-align: center;
  font-weight: 600;
}

.search .searchIpt {
  width: 56%;
  height: 100rpx;
  display: flex;
  align-items: center;
}

.search .searchIpt view {
  width: 80%;
  height: 70%;
  line-height: 70rpx;
  color: #FFF;
  background-color: #CCC;
  border-radius: 10rpx;
  padding-left: 30rpx;
  box-sizing: border-box;
  box-shadow: 5rpx 5rpx 5rpx 5rpx #999;
}

.homeBanner {
  width: 100vw;
  height: 350rpx;
  background-color: #FFF;
  padding-top: 130rpx;
}

.homeBanner swiper-item image {
  width: 100%;
  height: 100%;
}

.catebox {
  width: 100vw;
  height: 400rpx;
  padding: 18rpx;
  box-sizing: border-box;
  background-color: #FFF;
}

.catebox .catelist {
  width: 100%;
  height: 100%;
  background-color: #FFF;
  border-radius: 15rpx;
  box-shadow: 0 -3rpx 15rpx 10rpx #CCC;
  display: flex;
  flex-wrap: wrap;
}

.catebox .catelist .cate {
  width: 20%;
  height: 50%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.catebox .catelist .cate .cateimg {
  width: 100rpx;
  height: 100rpx;
}

.catebox .catelist .cate .cateimg image {
  width: 100%;
  height: 100%;
}

.zixun {
  width: 100vw;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #FFF;
  border-bottom: 1px solid #DDD;
  padding-bottom: 10rpx;
}

.zixun .zxtit {
  width: 30%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.zixun .zxtit view:nth-child(1) {
  font-size: 40rpx;
  font-weight: 700;
}

.zixun .zxtit view:nth-child(2) {
  font-size: 40rpx;
  color: #F00;
  font-weight: 700;
}

.zixun .zxbar {
  width: 70%;
}

.livebox {
  width: 100vw;
  height: 420rpx;
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #FFF;
}

.livebox .liveDB {
  width: 100%;
  height: 100%;
  background-color: #FFF;
  border-radius: 40rpx;
}

.livebox .liveDB image {
  border-radius: 40rpx;
  width: 100%;
  height: 100%;
}

.miaoshaTit {
  width: 100vw;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background-color: #FFF;
}

.miaoshaTit .miaoshaLine {
  width: 160rpx;
  border-top: 1px solid #CCC;
}

.miaoshaTit .miaoshaIcon {
  width: 30%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.miaoshaTit .miaoshaIcon image {
  width: 60rpx;
  height: 60rpx;
}

.miaoshabox {
  width: 100vw;
  padding: 20rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
  background-color: #FFF;
}

.miaoshabox .miaoshaGoods {
  width: 100%;
  height: 250rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 10rpx;
  margin-bottom: 40rpx;
}

.miaoshabox .miaoshaGoods .miaoshaGoodImg {
  width: 35%;
  height: 100%;
}

.miaoshabox .miaoshaGoods .miaoshaGoodInfo {
  width: 60%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.miaoshabox .miaoshaGoods .miaoshaGoodInfo .miaoshaGoodIntroduce {
  word-wrap: break-word;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  width: 100%;
  font-size: 28rpx;
}

.miaoshabox .miaoshaGoods .miaoshaGoodInfo .miaoshaJindu {
  height: 5%;
  width: 98%;
  background-color: rgba(255, 0, 0, 0.3);
  border-radius: 50rpx;
}

.miaoshabox .miaoshaGoods .miaoshaGoodInfo .miaoshaGoodBuy {
  width: 100%;
  height: 30%;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.miaoshabox .miaoshaGoods .miaoshaGoodInfo .miaoshaGoodBuy .miaoshaGoodPrice {
  width: 30%;
  font-size: 40rpx;
  color: red;
}

.miaoshabox .miaoshaGoods .miaoshaGoodInfo .miaoshaGoodBuy .miaoshaGoodOldPrice {
  width: 30%;
  font-size: 26rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
  color: #666;
  text-decoration: line-through;
}

.miaoshabox .miaoshaGoods .miaoshaGoodInfo .miaoshaGoodBuy button {
  width: 40%;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 50rpx;
  background-color: red;
  color: #FFF;
  font-size: 26rpx;
  font-weight: 400;
}

.baopinbox {
  margin-top: 20rpx;
